<update id="updateYn" parameterType="hashMap">
    UPDATE MEMBER
    SET MEM_CD = 
        CASE 
            WHEN #{memYn} = 'AYN01001' THEN 'MCC01002'
            WHEN #{memYn} IN ('AYN01002', 'AYN01003') THEN 'MCC01000'
            ELSE MEM_CD
        END,
        MEM_YN = #{memYn}
    WHERE MEM_ID = #{memId}
</update>

