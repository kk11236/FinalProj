<select id="getMember" parameterType="hashMap" resultType="memberVO">
	SELECT T.*
	FROM(
	SELECT ROW_NUMBER() OVER(ORDER BY Mem_NM) RNUM 
		  ,a.MEM_ID
		  ,a.MEM_PASS
		  ,a.MEM_NM
		  ,a.MEM_POSTNO
		  ,a.MEM_ADDR
		  ,a.MEM_ADDR2
		  ,a.MEM_TEL
		  ,a.MEM_REG_DT
		  ,a.MEM_OPEN_RESUME
		  ,a.MEM_STATE_CD
		  ,a.MEM_BIRTH
		  ,a.MEM_GEN
		  ,a.MEM_EMAIL
		  ,a.MEM_CD
		  ,a.MEM_YN
		  ,a.MEM_INTRCN
		  ,a.MEM_NCNM
		  ,a.MEM_AF_ID
		  ,a.MEM_WISH_JOB_CATE
		  ,a.MEM_WISH_JOB
		  ,a.ENABLED
		  ,a.MEM_INT_WORD
	FROM MEMBER a
	WHERE MEM_CD = 'MCC01000'
	AND MEM_STATE_CD != 'MSC01002'
	<include refid="where"></include>	
	) T
	WHERE 
		T.RNUM BETWEEN ((#{currentPage} - 1) * 10 + 1) AND ((#{currentPage} * 10))
</select>

